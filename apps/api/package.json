{
  "name": "@usex/api",
  "version": "1.0.0",
  "description": "Sync Store API service",
  "private": true,
  "author": {
    "name": "Ali Torki",
    "url": "https://github.com/ali-master",
    "email": "ali_4286@live.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ali-master/sync-store.git",
    "directory": "apps/api"
  },
  "engines": {
    "node": ">=23.11.0"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "nest build",
    "start": "nest start --env-file .env.local",
    "dev": "nest start --watch --env-file .env.local",
    "start:debug": "nest start --debug --watch --env-file .env.local",
    "start:prod": "node dist/main",
    "test:types": "tsc --noEmit",
    "test:knip": "knip",
    "test:knip:fix": "knip --fix",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "db:deploy": "prisma migrate deploy",
    "db:migrate": "prisma migrate dev",
    "db:setup": "pnpm db:deploy && pnpm db:seed:prune && pnpm db:seed:prepare",
    "db:view": "prisma studio",
    "db:generate": "prisma generate && prisma format",
    "db:push": "prisma db push",
    "db:reset": "prisma db push --force-reset",
    "db:seed": "prisma db seed --",
    "db:seed:prepare": "prisma db seed -- --prepare",
    "db:seed:prune": "prisma db seed -- --prune",
    "db:seed:patch": "prisma db seed -- --patch",
    "postinstall": "pnpm run db:generate",
    "preinstall": "npx is-ci || npx only-allow pnpm"
  },
  "dependencies": {
    "@fastify/compress": "^8.1.0",
    "@fastify/static": "^8.2.0",
    "@fastify/view": "^11.1.0",
    "@maxmind/geoip2-node": "^6.1.0",
    "@nestjs-cls/transactional": "^3.1.0",
    "@nestjs-cls/transactional-adapter-prisma": "^1.3.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-fastify": "^11.1.6",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/adapter-mariadb": "^6.14.0",
    "@prisma/client": "^6.14.0",
    "@usex/utils": "workspace:*",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "fastify": "^5.5.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.0",
    "kill-port": "^2.0.1",
    "nestjs-cls": "^6.0.1",
    "passport": "^0.7.0",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.1",
    "ramda": "^0.31.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "tiny-lru": "^11.4.5",
    "uncrypto": "^0.1.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/ramda": "^0.31.0",
    "@types/supertest": "^6.0.3",
    "@usex/tsconfig": "workspace:*",
    "@vitest/coverage-istanbul": "^3.2.4",
    "dotenv-expand": "^12.0.2",
    "knip": "^5.62.0",
    "prisma": "^6.14.0",
    "supertest": "^7.1.4",
    "tasuku": "^2.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "resolutions": {
    "fastify": "^5",
    "@fastify/middie": "^9",
    "@fastify/cors": "^10"
  },
  "packageManager": "pnpm@10.14.0"
}
