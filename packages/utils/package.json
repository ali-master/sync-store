{
  "name": "@usex/utils",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "module": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "assets"
  ],
  "directories": {
    "dist": "dist",
    "assets": "assets"
  },
  "scripts": {
    "dev": "tsup-node --watch",
    "start": "nodemon ./dist/index.js --watch",
    "build": "tsup-node",
    "prepublishOnly": "pnpm lint && pnpm test",
    "release": "pnpm run build",
    "postrelease": "standard-version",
    "release:beta": "cross-env CI=true pnpm run build",
    "postrelease:beta": "standard-version --prerelease beta",
    "test": "cross-env CI=true vitest --typecheck",
    "test:watch": "pnpm run test --watch",
    "test:ui": "pnpm run test:watch --ui --coverage.enabled=true",
    "test:bench": "pnpm run test bench",
    "test:coverage": "pnpm run test run --coverage",
    "test:debug": "vitest --inspect-brk --pool forks --poolOptions.forks.singleFork",
    "test:debug:watch": "pnpm run test:debug --watch --poolOptions.threads.isolate false",
    "inspect": "node-modules-inspector",
    "test:types": "tsc --noEmit",
    "test:knip": "knip",
    "test:knip:fix": "knip --fix",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "preinstall": "npx is-ci || npx only-allow pnpm"
  },
  "dependencies": {
    "@amaabca/sensitive-param-filter": "^1.2.10",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@noble/ciphers": "^1.3.0",
    "@orama/cuid2": "^2.2.3",
    "@persian-tools/persian-tools": "^3.6.0",
    "@supercharge/request-ip": "^1.2.0",
    "@visulima/humanizer": "^1.2.2",
    "ansi-regex": "^6.1.0",
    "cacheable-lookup": "^7.0.0",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.6.0",
    "exponential-backoff": "^3.1.2",
    "fastify-file-interceptor": "^1.0.9",
    "hibp": "^15.0.1",
    "ioredis": "^5.7.0",
    "ip-range-check": "^0.2.0",
    "isbot": "^5.1.29",
    "jsonpath-plus": "^10.3.0",
    "libphonenumber-js": "^1.12.10",
    "mailchecker": "^6.0.17",
    "maskdata": "^1.3.3",
    "nest-winston": "^1.10.2",
    "node-device-detector": "^2.2.2",
    "safe-stable-stringify": "^2.5.0",
    "stacktrace-parser": "^0.1.11",
    "type-fest": "^4.41.0",
    "uncrypto": "^0.1.3",
    "undici": "^7.13.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@usex/tsconfig": "workspace:*"
  },
  "peerDependencies": {
    "class-transformer": "*",
    "class-validator": "*",
    "effect": "^3.13.2",
    "express": "^4.21.2",
    "fastify": "^5.2.1",
    "ramda": "^0.30.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "packageManager": "pnpm@10.14.0"
}
